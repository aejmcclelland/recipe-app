{
    "compilerOptions": {
        "target": "ES2017",
        "lib": [
            "dom",
            "dom.iterable",
            "esnext"
        ],
        "allowJs": true,
        "skipLibCheck": true,
        "strict": false,
        "noEmit": true,
        "incremental": true,
        "module": "esnext",
        "esModuleInterop": true,
        "moduleResolution": "node",
        "resolveJsonModule": true,
        "isolatedModules": true,
        "jsx": "preserve",
        "plugins": [
            {
                "name": "next"
            }
        ],
        "baseUrl": ".", // Important for `@/` alias
        "paths": {
            "@/*": [
                "./*"
            ] // Maps `@/` to the root directory
        },
    },
    "include": [
        "next-env.d.ts",
        ".next/types/**/*.ts",
        "**/*.ts",
        "**/*.tsx"
, "utils/authOptions.ts", "app/recipes/page.jsx", "models/Category.js", "models/Recipe.js", "models/User.js", "components/RecipesClient.jsx", "app/recipes/profile/page.jsx", "components/RecipeOverviewCard.jsx"    ],
    "exclude": [
        "node_modules"
    ]
}